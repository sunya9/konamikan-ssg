{
  "name": "konamikan-ssg",
  "version": "1.0.0",
  "description": "generate static site for konamikan.",
  "author": "sunya9",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "pregenerate": "yarn run fetch",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "fetch": "ts-node --project tsconfig.node.json -r dotenv/config fetch",
    "indices": "ts-node --project tsconfig.node.json -r dotenv/config indices"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-build": "0.5.5",
    "@nuxt/typescript-runtime": "0.3.6",
    "@nuxtjs/amp": "0.2.1",
    "@nuxtjs/axios": "5.9.2",
    "@nuxtjs/bulma": "1.2.6",
    "@nuxtjs/pwa": "3.0.0-beta.19",
    "@tryghost/content-api": "1.3.5",
    "algoliasearch": "3.35.1",
    "cheerio": "1.0.0-rc.3",
    "css-mqpacker": "7.0.0",
    "dayjs": "1.8.18",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "node-fetch": "2.6.0",
    "node-sass": "4.13.0",
    "nuxt": "2.11.0",
    "nuxt-payload-extractor": "0.0.14",
    "nuxt-property-decorator": "2.5.0",
    "sass-loader": "8.0.0",
    "ts-node": "8.5.4",
    "vue-feather-icons": "5.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "2.0.0",
    "@nuxtjs/eslint-config-typescript": "1.0.0",
    "@nuxtjs/eslint-module": "1.1.0",
    "@types/algoliasearch": "3.34.7",
    "@types/cheerio": "0.22.15",
    "@types/lodash": "4.14.149",
    "@types/node-fetch": "2.5.4",
    "@types/tryghost__content-api": "1.3.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-nuxt": "0.5.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  }
}
